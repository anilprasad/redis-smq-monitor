    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start:dev": "run-p start:server start:webpack",
        "start:server": "TS_NODE_PROJECT='tsconfig-server.json' nodemon src/client/tools/ws-server-launcher.ts",
        "start:webpack": "webpack serve --progress --open",
        "prebuild": "npm run build:clean",
        "build": "run-p build:client build:server",
        "build:clean": "rimraf ./dist && mkdir ./dist",
        "build:client": "NODE_ENV=production webpack --progress",
        "build:server": "NODE_ENV=production tsc --project tsconfig-server.json"
    },
