{
    "name": "redis-smq-monitor",
    "description": "A monitoring tool for RedisSMQ message queue",
    "version": "2.0.0",
    "author": "Weyoss <weyoss@protonmail.com>",
    "license": "MIT",
    "keywords": [
        "redis",
        "message queue",
        "message",
        "queue",
        "job queue",
        "jobs",
        "redis-smq"
    ],
    "repository": {
        "type": "git",
